{
  "manifest_version": 2,
  "name": "Remind Me Not",
  "description": "This Chrome Extension will remind you to take a break from the computer",
  "version": "1.0",
  "browser_action": {
      "default_icon": "icons/alarm_icon.png",
      "default_popup": "/src/popup/popup.html"
  },
  "background": {
      "scripts": ["/src/background/background.js"]
  },
  "permissions": [
       "activeTab",
       "storage",
        "alarms"
   ]
}
