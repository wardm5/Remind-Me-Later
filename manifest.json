{
  "manifest_version": 2,
  "name": "Remind Me Not",
  "description": "This Chrome Extension will remind you to take a break from the computer",
  "version": "1.0",
  "icons": {
      "16":  "icons/icon16.png",
      "48":  "icons/icon48.png",
      "128": "icons/icon128.png"
  },
  "browser_action": {
      "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["background.js"]
  },
  "permissions": [
       "activeTab",
       "storage",
        "alarms"
   ]
}
